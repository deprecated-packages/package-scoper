{
    "name": "symplify/package-scoper",
    "description": "Scope package in unique namespace to prevent composer install conflicts - with 1 click",
    "license": "MIT",
    "bin": [
        "bin/package-scoper"
    ],
    "require": {
        "php": ">=7.3",
        "symfony/dependency-injection": "^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symplify/symplify-kernel": "^9.1",
        "symplify/composer-json-manipulator": "^9.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\PackageScoper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\PackageScoper\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
